<div class="container">
  <main class="grid">
    <% for (const pin of pins) { %>
      <article>
        <div class="text">
          <a href="/pins/<%= pin.id %>" class="card">
            <p class="text-link"><%= pin.description %></p>
          </a>
        </div>
        <div class="meta-text">
          <a href="/pins/<%= pin.id %>" class="title">
            <h3><%= pin.title %></h3>
          </a>
          <div class="icon-username">
            <i id="comment-icon" class="fa-solid fa-user"></i>
            <a href="/users/<%= pin.owner_id %>" class="username">
              <h5><%= pin.owner_name %></h5>
            </a>
          </div>
        </div>
      </article>
    <% } %>
  </main>
</div>
