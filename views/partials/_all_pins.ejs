<div class="container">
  <main class="grid">
    <% for (const pin of pins) { %>
      <article>
        <div class="text">
          <a href="/pins/<%= pin.id %>" class="card">
            <p><%= pin.description %></p>
          </a>
        </div>
        <div class="meta-text">
          <h3><%= pin.title %></h3>
          <div class="icon-username">
            <i id="comment-icon" class="fa-solid fa-user"></i><h5><%= pin.owner_name %></h5>
          </div>
        </div>
      </article>
    <% } %>
  </main>
</div>
